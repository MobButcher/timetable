{
  "presets": [
    "@babel/preset-env",
    [
      "@babel/preset-typescript",
      {
        "targets": "last 2 browsers, iOS 9",
        "useBuiltIns": "usage",
        "corejs": "3.6.5"
      }
    ]
  ],
	"plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread"
	]
}